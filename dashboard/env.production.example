# Production Environment Variables Example
# Copy this file to .env.production and fill in your values

VITE_API_BASE_URL=https://bakery-management-system-production.up.railway.app/api/
VITE_APP_NAME=Bakery Management System
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Google Maps API (if needed)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Analytics (optional)
VITE_GA_TRACKING_ID=your_ga_tracking_id_here 

# AI Chat Configuration
VITE_AI_CHAT_ENABLED=true
VITE_BOT_NAME=مساعد المخبز الذكي
VITE_MAX_MESSAGE_LENGTH=1000
VITE_ENABLE_SUGGESTED_QUESTIONS=true